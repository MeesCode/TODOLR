7) SELECT DISTINCT ToDoList.Id, ToDoList.Name FROM ToDoItem, ItemTag, Tag, ToDoList WHERE ToDoItem.ToDoListId=ToDoList.Id AND Tag.Id=ItemTag.TagId AND ItemTag.ToDoId=ToDoItem.Id AND Tag.Text="personal";
+----+----------+
| Id | Name     |
+----+----------+
|  1 | School   |
|  3 | Shared   |
|  4 | Personal |
+----+----------+

8) SELECT Tag.Text, SUM(Completed) AS completed, COUNT(*)-SUM(Completed) AS Pending FROM ItemTag JOIN Tag ON ItemTag.TagId=Tag.Id JOIN ToDoItem ON ToDoItem.Id=ItemTag.ToDoId GROUP BY Tag.Id;
+----------+-----------+---------+
| Text     | completed | Pending |
+----------+-----------+---------+
| webdata  |        11 |       2 |
| sport    |         1 |       2 |
| school   |        14 |       5 |
| work     |         0 |       2 |
| personal |         4 |       4 |
+----------+-----------+---------+

9)

10) SELECT Tag.text AS Tag, Datediff(ToDoItem.CompletionDate, ToDoItem.CreationDate) As Days FROM ToDoItem, Tag, ItemTag WHERE ToDoItem.Completed = 1 AND Tag.Id = ItemTag.TagId AND ItemTag.ToDoId = ToDoItem.Id GROUP BY Tag.Id, datediff(ToDoItem.CompletionDate, ToDoItem.CreationDate);
+----------+------+
| Tag      | Days |
+----------+------+
| webdata  |  -23 |
| webdata  |    0 |
| webdata  |    1 |
| webdata  |    2 |
| webdata  |    3 |
| webdata  |    4 |
| webdata  |   12 |
| webdata  |   14 |
| sport    |    7 |
| school   |  -23 |
| school   |    0 |
| school   |    1 |
| school   |    2 |
| school   |    3 |
| school   |    4 |
| school   |   12 |
| school   |   14 |
| school   |   41 |
| school   |   60 |
| personal |    3 |
| personal |    4 |
| personal |    7 |
| personal |    8 |
+----------+------+

11) SELECT a.TagId, b.TagId, COUNT(*) FROM ItemTag a INNER JOIN ItemTag b ON a.ToDoId=b.ToDoId WHERE a.TagId<>b.TagId AND a.TagId<b.TagId GROUP BY a.TagId, b.TagId;
+-------+-------+----------+
| TagId | TagId | COUNT(*) |
+-------+-------+----------+
|     1 |     3 |       13 |
|     1 |     5 |        1 |
|     2 |     5 |        3 |
|     3 |     5 |        2 |
+-------+-------+----------+

12) SELECT AVG(DATEDIFF(CompletionDate, CreationDate)) AS Average FROM ToDoItem WHERE ToDoListID=4;
+---------+
| Average |
+---------+
|  6.0000 |
+---------+

13)
SELECT Title FROM (
	(SELECT Title, DATEDIFF(CompletionDate, CreationDate) AS dif, temp.average FROM ToDoItem, (
		(SELECT ToDoListID, AVG(DATEDIFF(CompletionDate, CreationDate)) AS average FROM ToDoItem GROUP BY ToDoListID) AS temp
	)
	WHERE ToDoItem.ToDoListID=temp.ToDoListID) AS final
)
WHERE dif>average;
